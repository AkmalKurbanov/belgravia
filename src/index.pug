            a(data-fancybox="" data-src="#blockB" href="javascript:;")
              path.part(d="m 243.3449,127.97075 -0.34997,176.15091 42.81284,-0.58328 0.11665,6.53275 h 18.665 l 0.93325,-0.34997 -0.11665,-5.48284 6.41609,-0.58328 0.11665,16.7985 76.29318,-0.46663 0.11666,-7.11603 5.48284,0.34997 -0.23331,6.29944 19.7149,-0.11666 0.11666,-6.29944 12.01559,0.23332 v -9.21585 l 55.1784,0.11666 v 9.09919 l 12.83218,-0.46663 -0.46662,6.29944 19.36493,0.23331 -0.69994,-6.06612 h 7.23269 l -0.69994,6.76606 76.29318,-0.23331 v -15.63194 h 6.29943 v 6.53275 l 20.29819,-0.46662 v -5.5995 h 41.99624 V 127.38747 h -46.19587 l 0.23332,-5.36618 -17.26513,-0.46663 -0.46662,6.29944 h -4.66625 l -0.23331,-11.66562 -44.796,-0.46663 -0.23331,-5.83281 -18.665,0.46662 -0.46662,6.29944 h -12.59887 l 0.69993,10.26575 -5.13287,0.46662 -0.23331,-6.53275 h -19.13162 l 0.23331,6.53275 -82.12599,0.69994 -0.23331,-7.466 -17.49844,0.93325 -0.46662,6.29944 -5.36619,-0.23331 v -11.199 H 374.4665 l -0.23331,-6.53275 -19.36493,0.46662 -0.23332,6.06613 h -42.69618 l 0.23332,10.96568 h -4.43294 l 0.23331,-6.29943 -19.36493,-0.23332 0.23331,7.23269 z")
            a(data-fancybox="" data-src="#blockC" href="javascript:;")
              path.part(d="m 664.59055,127.50413 0.69994,177.20082 42.46287,-0.58329 0.11665,5.94947 20.5315,-0.34997 -0.69994,-5.71615 h 5.24953 l -0.11665,16.68184 78.15967,-0.58328 -0.46662,-7.93263 35.23018,0.46663 v -8.39925 h 2.09981 v 5.83281 h 17.49844 v -6.53275 l 17.49843,0.23332 0.23331,7.69931 17.03181,-0.93325 0.23332,-6.99938 1.63318,0.23332 0.23332,9.33249 36.39674,-0.23331 v 6.99938 l 76.99316,-0.23332 -0.4667,-16.33187 34.7636,0.46663 0.4666,6.29943 19.5983,0.23332 -0.4667,-6.53275 h 15.632 l -0.4667,-176.38423 h -12.1322 v -6.99937 l -16.5652,0.46662 0.2333,6.29944 -5.5995,0.23331 -0.4666,-12.13224 h -10.0324 l -0.2333,-6.29944 h -20.0649 l -0.2333,7.466 -19.1317,-0.93325 -0.2333,-6.29944 -17.96503,-0.23331 -0.46662,6.76606 -8.86588,0.23331 -0.93325,-6.53275 -20.76481,-0.23331 0.46663,6.53275 -13.29881,0.23331 -0.23331,11.89894 -6.29944,0.93325 v -7.466 l -17.49844,-0.46663 -0.23331,6.76607 -80.72611,0.69993 v -8.16593 l -17.96506,0.93325 v 7.466 l -5.13287,-0.46663 -0.23332,-13.0655 -13.99874,-0.23331 -0.23332,-5.5995 -20.53149,-0.23331 0.46662,6.29944 -48.06237,0.93325 0.23332,-6.76607 -20.76481,-0.23331 v 6.06613 l -9.79913,0.23331 0.69994,12.36556 h -3.26637 l 0.23331,-6.76606 -16.09856,-0.23332 -0.23331,6.53275 z")
          img(src="assets/img/plan/plantop.png" alt="")

  //- plan end

  //- popup-window
  .popup(style="display: none;" id="blockA")
    img(src="assets/img/plan/blocka.png", alt="")
  
  .popup(style="display: none;" id="blockB")
    img(src="assets/img/plan/blocka.png", alt="")
  
  .popup(style="display: none;" id="blockC")
    img(src="assets/img/plan/blocka.png", alt="")

  //- popup-window end


  include includes/footer/footer


  include includes/scripts 